{
  "type": "module",
  "scripts": {
    "prepare": "pika-web",
    "create:images": "node --experimental-modules create-images.js",
    "create:albums": "node --experimental-modules create-album-json.js",
    "create:zip": "node --experimental-modules create-zip.js",
    "build": "node --experimental-modules build.js",
    "build:watch": "node --experimental-modules build.js --watch",
    "serve": "serve ./_site/",
    "serve:dynamic": "node --experimental-modules server.js",
    "start": "npm run create:images && npm run create:albums && npm run create:zip && npm run build && npm run serve"
  },
  "engines": {
    "node": "12.13.x"
  },
  "dependencies": {
    "htm": "^2.2.1",
    "lit-element": "^2.2.1",
    "preact": "^10.0.4",
    "preact-render-to-string": "^5.1.1",
    "xstate": "^4.6.7"
  },
  "devDependencies": {
    "@pika/web": "^0.6.1",
    "archiver": "^3.1.1",
    "concurrent-transform": "^1.0.0",
    "connect-slashes": "^1.4.0",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "gulp": "^4.0.2",
    "gulp-image-resize": "^0.13.1",
    "js-beautify": "^1.10.2",
    "node-fetch": "^2.6.0",
    "serve": "^11.2.0"
  },
  "@pika/web": {
    "webDependencies": [
      "htm",
      "lit-element",
      "preact",
      "preact/hooks",
      "preact-render-to-string",
      "xstate"
    ]
  }
}
